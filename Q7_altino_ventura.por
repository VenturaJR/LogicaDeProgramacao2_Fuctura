programa {
	funcao inicio() 
	{
	    
	    real n1, n2, n3, mediaFinal
	    
	    escreva ("Digite o valor da primeira nota do aluno: ")
	    leia (n1)
	    escreva ("Digite o valor da segunda nota do aluno: ")
	    leia (n2)
	    escreva ("Digite o valor da terceira nota do aluno: ")
	    leia (n3)
	    
	    mediaFinal = (n1*2 + n2*3 + n3*5)/10
	    escreva ("A Média Final do Aluno foi: " +mediaFinal)
		
	}
}
